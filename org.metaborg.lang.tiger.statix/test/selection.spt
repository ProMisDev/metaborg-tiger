module renaming-variables

language Tiger


test select builtin entity  [[
let
	var msg := "Hello World"
in 
	[[print]](msg)
end
]] run rename(|#1, "message", "test/variables.spt") fails

test select literal  [[
let
	var msg := [["Hello World"]]
in 
	print(msg)
end
]] run rename(|#1, "message", "test/variables.spt") fails
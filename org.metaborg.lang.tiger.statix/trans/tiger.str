rules // Debugging

  debug-show-aterm:
    (node, _, _, path, project-path) -> (filename, result)
    with
      filename := <guarantee-extension(|"aterm")> path
    ; result   := node
    
rename-menu-action : menu-terms -> <rename-action(construct-textual-change|"static-semantics", "programOk")> menu-terms
